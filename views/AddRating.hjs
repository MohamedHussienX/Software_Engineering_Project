<!DOCTYPE html>
<html>
  <head>
    <head>
      <link rel='stylesheet' href='/styles/bootstrap.min.css' />
      <link rel='stylesheet' href='/styles/style.css' />
      <script src="/js/jquery-2.2.0.min.js" type="text/javascript"></script>
      <script src="/js/bootstrap.min.js" type="text/javascript"></script>
      <script src="/src/SubmitRating.js" type="text/javascript"></script>
      
    </head>
  </head>
  <body>
    <div>
       <nav class="navbar navbar-inverse" style="padding-left:130px;">
          <ul class="nav navbar-nav">
            <li><a href="/">Home</a></li>
            <li><a href="/Equipments">Equipments</a></li>
            <li class="active"><a href="/AddRating">AddRating</a></li>
            <li><a href="/Cart">Cart</a></li>
            <li><a href="/order">Order</a></li>
            <li><a href="/orderhistory">Order History</a></li>
          </ul>
      </nav>
    </div>
    <br/>
    <form class="form-horizontal" style="width: 80%;">
      <div class="form-group">
          <label for="fname" class="col-sm-2 control-label">Equipment Name</label>
          <div class="col-sm-10">
              <input type="text" class="form-control" id="equipmentname" name="fname" placeholder="Equipment Name...." value="{{name}}" required >
          </div>
      </div>
      <div class="form-group">
          <label for="country" class="col-sm-2 control-label">Score</label>
          <div class="col-sm-10">
              <input type="text" class="form-control" id="score" name="country" placeholder="Score" value="" required >
          </div>
      </div> 
      <div class="form-group">
          <label for="lname" class="col-sm-2 control-label">Comment</label>
          <div class="col-sm-10">
              <input type="text" class="form-control" id="comment" name="lname" placeholder="Comment...." value="" required >
          </div>
      </div>  
      <div class="form-group">
          <div class="col-sm-10 col-sm-offset-2">
              <input id="submit" name="submit" type="button" value="Submit" class="btn btn-primary">
          </div>
      </div>
  </form>
    </div>
  </body>
  </html>
